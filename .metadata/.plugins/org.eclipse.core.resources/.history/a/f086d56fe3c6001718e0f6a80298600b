package com.my.HRForecastCrawlTool;

/**
 * Created by Kunwar_HRForecast on 25-09-2017.
 */
public class CrawledDataServer {

    private String source_type;
    private String source_name;
    private double is_deleted;
    private String source_url;

    CrawledDataServer(String source_type, String source_name, double is_deleted, String source_url){
        this.setSource_type(source_type);
        this.setSource_type(source_name);
        this.setIs_deleted(is_deleted);
        this.setSource_url(source_url);
    }

    public String getSource_type() {
        return source_type;
    }

    public String getSource_name() {
        return source_name;
    }

    public void setSource_type(String source_type) {
        this.source_type = source_type;
    }

    public void setSource_name(String source_name) {
        this.source_name = source_name;
    }

    public double getIs_deleted() {
        return is_deleted;
    }

    public void setIs_deleted(double is_deleted) {
        this.is_deleted = is_deleted;
    }

    public String getSource_url() {
        return source_url;
    }

    public void setSource_url(String source_url) {
        this.source_url = source_url;
    }

}
